{
  "name": "open-element-template",
  "version": "0.0.2",
  "description": "Open element template",
  "main": "open-element.html",
  "keywords": [
    "web-component",
    "Polymer"
  ],
  "directories": {
    "test": "test",
    "demo": "demo"
  },
  "scripts": {
    "preinstall": "export CXX=\"/usr/bin/g++ -I/opt/local/include\"",
    "postinstall": "./node_modules/.bin/bower install",
    "demo": "bash -c 'sleep 1 && open http://localhost:8000/components/open-element/demo/index.html &' && ./node_modules/.bin/polyserve -p 8000",
    "test": "./node_modules/.bin/wct --simpleOutput --sauce default",
    "localtest": "./node_modules/.bin/wct --simpleOutput --local default",
    "testdebug": "./node_modules/.bin/wct -p -l chrome",
    "docs": "./node_modules/.bin/yuidoc --themedir ./node_modules/yuidoc-theme-blue -e .html,.js .",
    "testdocs": "/bin/bash -c 'sleep 2 && open http://localhost:5000 &'; ./node_modules/.bin/yuidoc --server 5000 -e .html,.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apowers313/open-element-template.git"
  },
  "author": "apowers@ato.ms",
  "license": "MIT",
  "loglevel": "info",
  "bugs": {
    "url": "https://github.com/apowers313/open-element-template/issues"
  },
  "homepage": "https://github.com/apowers313/open-element-template#readme",
  "dependencies": {
    "bower": "^1.5.2"
  },
  "devDependencies": {
    "polyserve": "^0.4.0",
    "web-component-tester": "^3.3.17",
    "yuidocjs": "^0.9.0",
    "travis-encrypt": "^1.1.1"
  }
}
